<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:HTMLPanel>
        <g:RootPanel>
            <g:Label>StockWatcher</g:Label>
            <g:VerticalPanel ui:field="mainPanel">
                <g:Label ui:field="errorMsgLabel"/>
                <g:FlexTable ui:field="stocksFlexTable">

                </g:FlexTable>
                <g:HorizontalPanel ui:field="addPanel">
                    <g:TextBox ui:field="newSymbolTextBox" focus="true"/>
                    <g:Button ui:field="addStockButton"/>
                </g:HorizontalPanel>
                <g:Label ui:field="lastUpdateLabel"/>
            </g:VerticalPanel>
        </g:RootPanel>
    </g:HTMLPanel>
</ui:UiBinder>